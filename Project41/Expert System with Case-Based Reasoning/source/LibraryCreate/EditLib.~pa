unit EditLib;

interface

uses
  Windows, Messages, SysUtils, Classes, Graphics, Controls, Forms, Dialogs,
  StdCtrls;

type
  TEditForm = class(TForm)
    Label1: TLabel;
    CaseNo: TEdit;
    Feature: TEdit;
    Label2: TLabel;
    SolNo: TEdit;
    OkButton: TButton;
    ShowButton: TButton;
    CancelButton: TButton;
    PreviousButton: TButton;
    NextButton: TButton;
    procedure CancelButtonClick(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  EditForm: TEditForm;

implementation

{$R *.DFM}






procedure TEditForm.CancelButtonClick(Sender: TObject);
begin
     EditForm.close;
end;

end.
