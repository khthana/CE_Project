program SolutionCreator;

uses
  Forms,
  SolMain in 'SolMain.pas' {SolForm},
  LoadSol in 'LoadSol.pas' {LoadForm},
  SaveSol in 'SaveSol.pas' {SaveForm};

{$R *.RES}

begin
  Application.Initialize;
  Application.CreateForm(TSolForm, SolForm);
  Application.CreateForm(TLoadForm, LoadForm);
  Application.CreateForm(TSaveForm, SaveForm);
  Application.Run;
end.
