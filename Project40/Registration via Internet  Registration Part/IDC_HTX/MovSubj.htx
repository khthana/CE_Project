<html>
<head>
<!-- The Function of this file is amout 'x' at first Id for that id is to move -->
<script language="JavaScript">
function AssignValueToElements() {
	for(i=3; i<document.formx.elements.length-13; i++){ /* start at first Subj to end Subj*/
		document.formx.elements[i].value='x'+document.formx.elements[i].value;
	}
}
function AssignValueToSubj(){
	n=document.formx.elements.length-13;
	for(i=n; i<=n+9; i++) /* from Subj0..Subj9 */
		document.formx.elements[i].value='xx'
}
function ChangeValue(ChkBoxValue){ /* Change Value for each check box */
	for(i=3; i<document.formx.elements.length-13; i++){
		if(document.formx.elements[i].value==ChkBoxValue){
			document.formx.elements[i].value='x'+ChkBoxValue;
			i=document.formx.elements.length;
		}
		else{
			if(document.formx.elements[i].value==('x'+ChkBoxValue)){
				document.formx.elements[i].value=ChkBoxValue;
				i=document.formx.elements.length;
			}
		}
	}
}	
function SendInfo(){
	var str=document.formx.Std_Id.value;
	var nSubj=document.formx.elements.length-13; /*start location of subj0..subj9 */
	document.formx.LoginName.value='s'+str.substring(1, 8); /* for s8013289 */
	AssignValueToSubj();  /* to Assign 'xx' to Subject */
	for(i=3; i<document.formx.elements.length-13; i++)
		 /* copy value from checkbox to hidden elements before send*/
		if(document.formx.elements[i].value.charAt(0) != 'x'){
		  document.formx.elements[nSubj].value=document.formx.elements[i].value;
		  nSubj++;
		}
}
/* Verify function is Option for check the Subject */
function verify(theForm) {
	SendInfo(); /* call for set value to parameter before send to*/
	haveSubj=false;
	strOut="แสดงวิชาหลังจากการถอนรายวิชา : \n\n";
	for(i=3; i<theForm.elements.length-13; i++) { /* Start and end show at Subject */
		if(theForm.elements[i].checked){
			s_value = theForm.elements[i].value;
			/* get subject id only */
			strOut += "ลำดับที่ "+(i-2)+" : "+s_value.substring(1,9)+"\n";
			haveSubj=true; /* status tell for have Subject not remove all */
		}
		else	strOut += "ลำดับที่ "+(i-2)+" : "+theForm.elements[i].value+"  *** ถอน ***\n";
		/*strOut += theForm.elements[i].name + " : "+"\t" + theForm.elements[i].value + " \t\n";*/
	}
	strOut += "\nรายวิชาถูกต้องหรือไม่ ?";

	if( haveSubj==false ){ /* if remove all of Subject */
		alert ( "  ไม่สามารถถอนทุกรายวิชาได้ !  ");
		return false;
	}
	else {
		/* copy password */
		document.formx.Signature.value=parent.left.document.c4.Passwd.value;
		return confirm(strOut);
	}
}
</script>
</head>
<body background=..\..\Image\Bg_brown.gif onLoad="AssignValueToElements()">
<form name=formx action="http://161.246.6.32/scripts/Project97/toMov.idc"
	method=post onSubmit="return verify(this)">
