			 ?samplesize is 'n > 30'
			then TypeOfTesting  is  'Z - test for two Populations - using Independent sampling'  and ruleno is 16.

			(* Rule 17*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Proportion and
			 ?independence is 'Paired Comparison' and
			 ?distribution is Normal and
			 ?variance_known is Known
			then TypeOfTesting  is  'Z - test for two Normal, known Variance  Populations - using Paired Comparison sampling.'  and ruleno is 17.

			(* Rule 18*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Proportion and
			 ?independence is 'Paired Comparison' and
			 ?distribution is Normal and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for two Populations - using Paired comparison sampling.'  and ruleno is 18.

			(* Rule 19*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Proportion and
			 ?independence is Independent and
			 ?distribution is Normal and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for two Populations - using Independent sampling'  and ruleno is 19.

			(* Rule 20*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Proportion and
			 ?independence is Independent and
			 ?distribution is 'Not Normal' and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  'Nonparametric Test'  and ruleno is 20.

			(* Rule 21*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Proportion and
			 ?independence is 'Paired Comparison' and
			 ?distribution is 'Not Normal' and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  'Non-Parametric Test'  and ruleno is 21.

			(* Rule 22*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'More than two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is Independent and	
			 ?distribution is Normal  and
			 ?equal_variance is Equal
			 then TypeOfTesting  is  'ANOVA  test '  and ruleno is 22.

			(* Rule 23*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'More than two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is Independent   and	
			 ?distribution is 'Not Normal' 
			 then TypeOfTesting  is  'Non-Parametric ANOVA  test'  and ruleno is 23.

			(* Rule 24*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'More than two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is 'Paired Comparison' and	
			 ?distribution is 'Not Normal'
			 then TypeOfTesting  is  'Non-Parametric ANOVA  test '  and ruleno is 24.

			(* Rule 25*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'More than two Populations' and
			 ?KindOfTest2 is Mean and
			 ?distribution is Normal and
			 ?equal_variance is 'Not equal' 	
			 then TypeOfTesting  is  'Non-Parametric ANOVA  test '  and ruleno is 25.
			(* Rule 26*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'More than two Populations' and
			 ?KindOfTest2 is Mean and
			 ?distribution is Normal and
			 ?equal_variance is 'Not equal'
			 then TypeOfTesting  is  'Non-Parametric ANOVA  test '  and ruleno is 26.


			topic independence.
				ask('The Sampling is taken on the way of : ',independence,[Independent, 'Paired Comparison']).
			end. (* independence*)
			
			topic equal_variance.
				ask('  *** Note ****
   The following is question concerning assumption about the equality of variances.
    The Populations Variances are : ' ,equal_variance, [Equal, 'Not equal']).
			end. (* equal_variance *)

			topic distribution.
				ask('What are the population distributions ?',distribution,[Normal,'Not Normal',Unknown]).
			end. (* distribution *)

			topic NumberPopulation.
				ask ('What is the number of Population ? ',NumberPopulation , ['One Population','Two Populations', 'More than two Populations']).			
			end. (* NumberPopulation*)

			topic KindOfTest1.
				ask ('Which kind of Parameters  do you want to test : ', KindOfTest1, [Population, Distribution, 'Relationship between Variables']).
			end. (* KindOfTest1*)

			topic KindOfTest2.
			        (*	KindOfTest2 is Mean.*)
			        ask ('Which kind of Parameters  do you want to test : ', KindOfTest2, [Mean, Proportion, Variance]). 
			           (*	radio_button([ [Mean,10,6], [ proportion,10,8], [Variance, 10,10] ], KindOfTest2). 
				button(OK,continue_ve,10,9).
				wait(). *)
			end. (* KindOfTest2*)

			topic variance_known.
				ask ('The Variance of Population are : ', variance_known, [Known, UnKnown]).
			end. (* variance_known*)

			topic samplesize.
				ask ('What is the Size of sample taken ? ', samplesize, ['n <= 30', 'n > 30']).
			end. (*  samplesize*)
		end. (* TypeOfTesting*)

(*		text ('#n#n#n Press button OK to come back the main Menu').
		button (OK, :Next,  15,9).
		topic Next.
			close_window().
		end. (*Next*)
*)
	end. (* &Consultant*)

	topic &Testing.

		run('graph.exe test.txt',1).
	end.

