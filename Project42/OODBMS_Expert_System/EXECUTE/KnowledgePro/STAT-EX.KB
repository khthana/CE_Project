 topic GetOption (item).
	 do (?item).
	reset([&Consultant, &Testing,&Separately_Testing]).

	topic &Quit.
		exit_kp().
	end.
	topic &Separately_Testing.
		if NOT(exists(w1)) 
		 then w1 is window (,5,4,80,15,'Working Window',[overlapped,disabled,thickFrame] ).
		 window(?w1). 
		read_response('#n Give the kind of test you want to conduct #n( 0-Ztest, 1-t-test, 2-F test, 3-Chi-square) : ', nhan0,0).
		vl0 is topic_value(nhan0).
		read_response('#nGive the value of confidence level alpha : ',nhan1,'0.05').
		vl1 is topic_value(nhan1).

		if ?vl0 is 2 or ?vl0 is 3
		then vl2 is 1
		else 	read_response('#nTesting for 1 or 2 tailed ? ',nhan2,2) and
			vl2 is topic_value(nhan2).

		(* read_response('#nGive the value of statistic:  ',nhan3, 0.0). *)

		(* Read value of statistics from file        statis.txt          *)
		(* the value of test-statistic given by dtest.exe program *)

		 run('dtest.exe',1). 
		vl3 is read_line('statis.txt',1).

		if ?vl0 is 3 
		then vl0 is 2. (* Chi-square *)

		if ?vl0 is 0 then vl4 is 0
		else 	read_response('#nGive the value of degree of freedom ( 0 - for Ztest):  ',nhan4, ) and
			vl4 is topic_value(nhan4).

		new_file('test.txt').
		write('test.txt',?vl0). (* value of 0 : Z-test *)
		write('test.txt','#n',?vl1).
		write('test.txt','#n',?vl2).
		write('test.txt','#n',?vl4). 
		write('test.txt','#n',?vl3).
		close('test.txt').
		run ('graph.exe test.txt',1).
		reset([&Consultant, &Testing,&Separately_Testing]).	
		close_window().
	end.

	topic &Consultant. 
		if NOT(exists(w1)) 
		 then w1 is window (,5,4,80,15,'Working Window',[overlapped,disabled,thickFrame] ).
		type_explain=?TypeOfTesting.
		text ('#e The type of Testing Hypothesis should be:   #s#fred', ?type_explain).

		if ?type_explain is 'Z - test for one Population'
		then do (hoi1).

		if ?type_explain is 't - test for one Population'
		then do (hoi2).

		if ?type_explain is 'Z - test for one Population- Large Sample size'
		then do (hoi1).

		if ?type_explain is 'Z - test for two Populations - using Paired comparison.'
		then do (hoi1).

		if ?type_explain is 'Z - test for two Populations - using Independent sampling.'				
		then do (hoi1).

		if ?type_explain is 'ANOVA  test '
		then do (hoi6).

		if ?type_explain is 'Non Parametric ANOVA  test '							
		then do (hoi7).

		if ?type_explain is 'Z - test for two Normal, known Variance  Populations - using Paired Comparison sampling.'
		then do (hoi1).

		if ?type_explain is 't - test for two Populations - using Paired comparison.'
		then do (hoi2).

		if ?type_explain is 't - test for two Populations - using Independent sampling'
		then do (hoi2).



		text('#n#n#n Do you want to know WHY  I give you this recommendation ?').
		bt_ask is button(No,retu,15,9,,t).
		button (Yes,retu).
		set_focus(?bt_ask).

		topic retu(item).
			if  ?item is No
			     then close_window()
			 else
			    do (explain_TypeOfTesting).

			topic explain_TypeOfTesting.

				if ?ruleno is 1 
				then 	do (exp1).
				if ?ruleno is 2
				then do (exp2).

				if ?ruleno is 3
				then do (exp3).

				if ?ruleno is 4
				then do (exp4).

				if ?ruleno is 5			
				then do (exp5).

				if ?ruleno is 6
				then do (exp6).

				if ?ruleno is 7						
				then do (exp7).

				if ?ruleno is 10
				then do (exp10).

				if ?ruleno is 11
				then do (exp11).
				
				if ?ruleno is 20
				then do (exp20).
				if ?ruleno is 21
				then do (exp21).
				if ?ruleno is 22
				then do (exp22).
				if ?ruleno is 23
				then do (exp23).
				if ?ruleno is 24
				then do (exp24).
				if ?ruleno is 25
				then do (exp25).
				if ?ruleno is 26
				then do (exp26).
				(*  Explanation *)

				topic exp1.
					(* Rule 1*)
					text('#e Because you have answered me that:
	- You want to test about #fred population#d
	- #fred And #dnumber of population is #fred One #d
	- #fred And #d the parameter you want to test is #fred Mean of Population#d
	- #fred And #d sample size is #fred bigger than 30#d. #nSo the Z-test is appropriate. (Applying Central Limit Theorem) ').
					reset([type_explain,&Consultant,TypeOfTesting]).
					button(OK,continue_ve,10,14).
					wait().			
					do (continue_ve).
				end. (*exp1*)

				topic exp2.
					(* Rule 2*)
					text('#e Because you have answered me that:
	- You want to test about #fred population#d
	- #fred And #dnumber of population is #fred One #d
	- #fred And #d the parameter you want to test is #fred Mean of Population#d
	- #fred And #d the distribution is #fred Normal#d and sample size is #fred less than 30#d. #nSo the t-test is appropriate. ').
					reset([type_explain,&Consultant,TypeOfTesting]).
					button(OK,continue_ve,10,14).
					wait().			
					do (continue_ve).
				end. (*exp2*)

				topic exp3.
					(* Rule 3*)
					text('#e Because you have answered me that:
	- You want to test about #fred population#d
	- #fred And #dnumber of population is #fred One #d
	- #fred And #d the parameter you want to test is #fred Mean of Population#d
	- #fred And #d the distribution is #fred Normal#d and sample size is #fred less than 30#d.
	  #nSo the Non-Parametric  test for one Population. ').
					reset([type_explain,&Consultant,TypeOfTesting]).
					button(OK,continue_ve,10,14).
					wait().			
					do (continue_ve).
				end. (*exp3*)		


				topic exp4.
					(* Rule 4*)
					text('#e Because you have answered me that:
	- You want to test about #fred population#d
	- #fred And #dnumber of population is #fred Two #d
	- #fred And #d the parameter you want to test is #fred Mean of Population#d
	- #fred And #d the distribution is #fred Normal#d and sample size is #fred less than 30#d
