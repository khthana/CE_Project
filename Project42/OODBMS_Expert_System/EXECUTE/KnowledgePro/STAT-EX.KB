				topic continue_ve.	
					close_window().
				end. (*continue_ve*).	
				reset([&Consultant,TypeOfTesting]).
			end. (*explain_TypeOfTesting*)
		end. (*retu*)

		(* For Z-test only *)
		topic hoi1.
			read_response('#n#nGive the value of confidence level alpha : ',nhan1,'0.05').
			vl1 is topic_value(nhan1).
			read_response('#nTesting for 1 or 2 tailed ? ',nhan2,2).
			vl2 is topic_value(nhan2).
	
			(* read_response('#nGive the value of Z-statistic:  ',nhan3, ). *)
			 run('dtest.exe',1). 
			vl3 is read_line('statis.txt',1).
	
			new_file('test.txt').
			write('test.txt',0). (* value of 0 : Z-test *)
			write('test.txt','#n',?vl1).
			write('test.txt','#n',?vl2).
			write('test.txt','#n',2). 
			write('test.txt','#n',?vl3).
			close('test.txt').
		end. (* set values of test*)

		(* For t-test only *)
		topic hoi2.
			read_response('#n#nGive the value of confidence level alpha : ',nhan1,'0.05').
			vl1 is topic_value(nhan1).
			read_response('#nTesting for 1 or 2 tailed ? ',nhan2,2).
			vl2 is topic_value(nhan2).

			(* read_response('#nGive the value of t-statistic:  ',nhan3, ). *)
			 run('dtest.exe',1). 
			vl3 is read_line('statis.txt',1).
	
			read_response('#nGive the value degree of freedom:  ',nhan4, ).
			vl4 is topic_value(nhan4).

			new_file('test.txt').
			write('test.txt',1). (* value of 1 : t-test *)
			write('test.txt','#n',?vl1).
			write('test.txt','#n',?vl2).
			write('test.txt','#n',?vl4). 
			write('test.txt','#n',?vl3).
			close('test.txt').
		end. (* set values of test*)

		(* For 'Z - test for one Population- Large Sample size' only *)
		topic hoi3.
			read_response('#n#nGive the value of confidence level alpha : ',nhan1,'0.05').
			vl1 is topic_value(nhan1).
			read_response('#nTesting for 1 or 2 tailed ? ',nhan2,2).
			vl2 is topic_value(nhan2).

			(* read_response('#nGive the value of Z-statistic (Computed from the formulation of Proportion):  ',nhan3, ). *)
			 run('dtest.exe',1). 
			vl3 is read_line('statis.txt',1).
			new_file('test.txt').
			write('test.txt',0). (* value of 0 : Z-test *)
			write('test.txt','#n',?vl1).
			write('test.txt','#n',?vl2).
			write('test.txt','#n',2). 
			write('test.txt','#n',?vl3).
			close('test.txt').
		end. (* set values of test*)


		topic TypeOfTesting.
			 window(?w1). 
			set_number_of_values (TypeOfTesting,1).
	
			(* Rule 0-1: For 1 population, mean, vaiance known*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Mean and
			 ?distribution is 'Not Normal'  or ?distribution is Normal and
			 ?variance_known is Known or
			 ?variance_known is UnKnown and
			 ?samplesize is 'n > 30'
			then TypeOfTesting  is  'Z - test for one Population' and ruleno is 1.

			(* Rule 0-2: For 1 population, mean, vaiance known*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Mean and
			 ?distribution is Normal and
			 ?variance_known is Known or
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for one Population' and ruleno is 2.


			(* Rule 3: For 1 population, mean, vaiance known*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Mean and
			 ?distribution is 'Not Normal' and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  'Non-Parametric  test for one Population' and ruleno is 3.
			
			(* Rule 4*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Mean and
			 ?distribution is Normal and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for one Population' and ruleno is 4.

			(* Rule 5*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Mean and
			 ?samplesize is 'n > 30'
			then TypeOfTesting  is  'Z - test for one Population- Large Sample size'  and ruleno is 5.

			(* Rule 6*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Proportion and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n > 30'
			then TypeOfTesting  is  'Z - test for one Population- Large Sample size'  and ruleno is 6.

			(* Rule 7*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'One Population' and
			 ?KindOfTest2 is Proportion and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for one Population- Small Sample size'  and ruleno is 7.


			(* Rule 10*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is 'Paired Comparison' and
			 ?distribution is Normal or
			 ?distribution is 'Not Normal' or		
			 ?variance_known is Known or
			?variance_known is UnKnown and	 
			 ?samplesize is 'n > 30'
			then TypeOfTesting  is  'Z - test for two Populations - using Paired comparison.'  and ruleno is 10.

			(* Rule 11*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is Independent and
			 ?distribution is Normal or
			  ?distribution is 'Not Normal' or
			 ?variance_known is Known or
			 ?variance_known is UnKnown and
			 ?samplesize is 'n > 30'
			then TypeOfTesting  is  'Z - test for two Populations - using Independent sampling.'  and ruleno is 11.

			(* Rule 12*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is 'Paired Comparison' and
			 ?distribution is Normal and
			 ?variance_known is Known
			then TypeOfTesting  is  'Z - test for two Normal, known Variance  Populations - using Paired Comparison sampling.'  and ruleno is 12.

			(* Rule 13*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is 'Paired Comparison' and
			 ?distribution is Normal and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for two Populations - using Paired comparison.'  and ruleno is 13.

			(* Rule 14*)
			if ?KindOfTest1 is Population and
			 ?NumberPopulation is 'Two Populations' and
			 ?KindOfTest2 is Mean and
			 ?independence is Independent and
			 ?distribution is Normal and
			 ?variance_known is UnKnown and
			 ?samplesize is 'n <= 30'
			then TypeOfTesting  is  't - test for two Populations - using Independent sampling'  and ruleno is 14.

			(* Rule 15*)
			if ?KindOfTest1 is Population and
